<html>
<head>
    <meta charset="UTF-8">
    <title>Resume - {{ resume.name }}</title>
    <style type="text/css">
        .print-only {
            visibility: hidden;
            display: none;
        }
        @page {
            size: a4;
        }
        @media print {
            .print-only {
                visibility: visible;
                display: unset;
            }
        }

        html {
            font-family: 'Times New Roman';
        }

        .title {
            padding-top: 24pt;
            color: #000000;
            font-weight: 700;
            font-size: 36pt;
            padding-bottom: 6pt;
            line-height: 1;
            page-break-after: avoid;
            orphans: 2;
            widows: 2;
            text-align: left;
        }

        sup {
            font-size: 5pt;
            font-family: inherit;
        }

        .subtitle {
            padding-top: 18pt;
            color: #666666;
            font-size: 24pt;
            padding-bottom: 4pt;
            line-height: 1;
            page-break-after: avoid;
            font-style: italic;
            orphans: 2;
            widows: 2;
            text-align: left;
        }

        body {
            background-color: #ffffff;
            max-width: 500pt;
            padding: 40pt;
            margin: auto;
        }

        a {
            color: inherit;
            text-decoration: none;
        }

        a:hover, a:hover span {
            color: #3cb043;
            text-decoration: underline;
        }

        ul {
            padding: 0;
            margin: 0;
        }

        p {
            margin: 0;
            color: #000000;
            text-decoration: none;
            vertical-align: baseline;
            font-style: normal;
            font-size: 10pt;
            font-family: inherit;
            font-weight: 400;
        }

        h1 {
            margin: 8px 0;
            text-transform: uppercase;
            letter-spacing: 3.2pt;
            font-size: 16pt;
            color: #000000;
            text-decoration: none;
            vertical-align: baseline;
            font-style: normal;
            padding-top: 0pt;
            padding-bottom: 0pt;
            line-height: 1.15;
            orphans: 2;
            widows: 2;
            text-align: center;
        }

        h2 {
            text-transform: uppercase;
            margin: 8px 0;
            letter-spacing: 3pt;
            color: #000000;
            text-decoration: none;
            vertical-align: baseline;
            font-style: normal;
            font-size: 9pt;
            font-family: inherit;
            font-weight: 400;
            text-align: center;
        }

        h3 {
            text-transform: uppercase;
            margin: 8px 0;
            letter-spacing: 1.3pt;
            font-size: 9pt;
            font-family: inherit;
            font-weight: 500;
            line-height: 1.15;
            border-bottom-color: #000000;
            border-bottom-width: 0.5pt;
            padding-bottom: 1pt;
            line-height: 1.15;
            border-bottom-style: solid;
            orphans: 2;
            widows: 2;
            text-align: center;
        }

        h4 {
            text-transform: capitalize;
            margin: 1px 0;
            font-weight: 400;
            font-size: 11pt;
            font-family: inherit;
            vertical-align: baseline;
            padding-top: 0pt;
            padding-bottom: 0pt;
            line-height: 1.15;
            orphans: 2;
            widows: 2;
            text-align: left;
        }

        h5 {
            text-transform: capitalize;
            margin: 2px 0;
            padding-top: 0pt;
            padding-bottom: 0pt;
            line-height: 1.15;
            orphans: 2;
            widows: 2;
            text-align: left;
            font-size: 10pt;
            font-family: inherit;
            font-weight: 700;
        }

        h6 {
            padding-top: 10pt;
            color: #000000;
            font-weight: 700;
            font-size: 10pt;
            padding-bottom: 2pt;
            font-family: 'Times New Roman';
            line-height: 1;
            page-break-after: avoid;
            orphans: 2;
            widows: 2;
            text-align: left;
        }

        ol.footnotes {
            margin-left: -2pt;
            padding-inline-start: 12px;
        }

        span.endnote {
            color: #000000;
            text-decoration: none;
            vertical-align: baseline;
            font-style: normal;
            font-size: .5em;
            font-family: inherit;
            vertical-align: top;
        }
        ol.footnotes li {
            color: #000000;
            text-decoration: none;
            vertical-align: baseline;
            font-style: normal;
            font-size: 7pt;
            font-family: inherit;
        }

        ul.footnotes li:before {
            margin-left: -2pt;
        }

        ul.compact {
            font-size: 0;
            text-align: center;
        }

        ul.compact li {
            color: #000000;
            text-decoration: none;
            vertical-align: baseline;
            font-style: normal;
            font-size: 10pt;
            font-family: inherit;
            font-weight: 400;
            display: inline;
        }

        ul.compact li:before {
            content: "|";
            padding-right: 8px;
            padding-left: 8px;
        }

        ul.simple {}

        ul.simple li:before {
            margin-left: -18pt;
            white-space: nowrap;
            display: inline-block;
            min-width: 18pt;
        }

        ul.simple li {
            color: #000000;
            padding-top: 0pt;
            padding-bottom: 0pt;
            line-height: 1.15;
            orphans: 2;
            widows: 2;
            text-align: left;
            margin-left: 36pt;
            padding-left: 0pt;
            text-decoration: none;
            vertical-align: baseline;
            font-style: normal;
            font-size: 10pt;
            font-family: inherit;
            font-weight: 400;
        }

        .timeline {
            margin-top: 8px;
            text-align: right;
            color: #000000;
            vertical-align: baseline;
            font-style: normal;
            font-size: 10pt;
            font-family: inherit;
            font-weight: 400;
        }

        .footer {
            font-style: normal;
            font-size: 8pt;
            font-family: inherit;
            color: #000000;
            text-decoration: none;
            vertical-align: baseline;
            text-align: right;
        }

        .center-container {
            margin: 0 auto;
            position: relative;
            width: 100%;
        }

        span.divider {
            margin-left: .5em;
            margin-right: .5em;
        }

        table {
            width: 100%;
            border-collapse: collapse;
            border: none;
        }

        td {
            vertical-align: top;
            border: none;
        }
        .left {
            width: 23%;
        }

        .right {
            width: 77%;
        }
        @media print {
            .print-button {
                display: none;
            }
        }
    </style>
</head>
<body>
    <div>
     <button class="print-button" onclick="printPage()">Print to PDF</button>
     <script>
        function printPage() {
            window.print();
        }
    </script>
    </div>
    <table>
        <tbody>
            <tr>
                <td>
                    {% block title %}{% endblock title %}
                    {% block current %}{% endblock current %}
                    {% block professionl_experience %}{% endblock professionl_experience %}
                    {% block projects %}{% endblock projects %}
                    {% block certification %}{% endblock certification %}
                    {% block education %}{% endblock education %}
                    {% block skills %}{% endblock skills %}
                </td>
            </tr>
        </tbody>
    </table>
</body>
<footer>
<table>
    <tbody>
        <tr>
            <td>
                {% block footer %}{% endblock footer %}
            </td>
        </tr>
    </tbody>
</table>
</footer>
</html>
